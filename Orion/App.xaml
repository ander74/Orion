<Application
    x:Class="Orion.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:Orion"
    d1p1:Ignorable="d"
    StartupUri="Views\MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Estilos/Colores.xaml" />
                <ResourceDictionary Source="Estilos/Fondos.xaml" />
                <ResourceDictionary Source="Estilos/Filas.xaml" />
                <ResourceDictionary Source="Estilos/Celdas.xaml" />
                <ResourceDictionary Source="Estilos/OtrosEstilos.xaml" />
                <ResourceDictionary Source="Estilos/Cabeceras.xaml" />
                <ResourceDictionary Source="Estilos/QDatagrid.xaml" />
                <ResourceDictionary Source="Estilos/QControlsStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>