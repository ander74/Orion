<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Orion">


    <!--  ====================================================================================================  -->
    <!--  FONDOS  -->
    <!--  ====================================================================================================  -->
    <SolidColorBrush x:Key="FondoCeleste1" Color="{StaticResource Celeste1}" />
    <SolidColorBrush x:Key="FondoCeleste2" Color="{StaticResource Celeste2}" />
    <SolidColorBrush x:Key="FondoCeleste3" Color="{StaticResource Celeste3}" />
    <SolidColorBrush x:Key="FondoCeleste4" Color="{StaticResource Celeste4}" />
    <SolidColorBrush x:Key="FondoCeleste5" Color="{StaticResource Celeste5}" />
    <SolidColorBrush x:Key="FondoCeleste6" Color="{StaticResource Celeste6}" />

    <!--  ====================================================================================================  -->
    <!--  DEGRADADOS  -->
    <!--  ====================================================================================================  -->
    <LinearGradientBrush x:Key="DegradadoCeleste0" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="{StaticResource Celeste1}" />
        <GradientStop Offset="1" Color="{StaticResource Blanco}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DegradadoCeleste1" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="{StaticResource Celeste2}" />
        <GradientStop Offset="1" Color="{StaticResource Celeste1}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DegradadoCeleste2" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="{StaticResource Celeste3}" />
        <GradientStop Offset="1" Color="{StaticResource Celeste2}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DegradadoNaranja" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="{StaticResource NaranjaMuyClaro}" />
        <GradientStop Offset="1" Color="{StaticResource Blanco}" />
    </LinearGradientBrush>





    <!--  ====================================================================================================  -->
    <!--  DEGRADADOS VERDES  -->
    <!--  ====================================================================================================  -->
    <LinearGradientBrush x:Key="VerdeMuyClaro" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#cefddc" />
        <GradientStop Offset="0.3" Color="#effffe" />
        <GradientStop Offset="1" Color="#cefddc" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VerdeClaro" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#9fffbc" />
        <GradientStop Offset="1" Color="#57df80" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VerdeMedio" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#57ff73" />
        <GradientStop Offset="1" Color="#00be1f" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VerdeOscuro" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#4de266" />
        <GradientStop Offset="1" Color="#028b19" />
    </LinearGradientBrush>


